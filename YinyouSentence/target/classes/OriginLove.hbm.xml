<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.OriginLove" table="origin_love" schema="yinyousentence">
        <id name="id">
            <column name="id" sql-type="bigint(20) unsigned"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="originId">
            <column name="origin_id" sql-type="bigint(20) unsigned"/>
        </property>
    </class>
</hibernate-mapping>