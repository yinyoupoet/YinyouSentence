<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.SentenceCategory" table="sentence_category" schema="yinyousentence">
        <id name="id">
            <column name="id" sql-type="bigint(20) unsigned"/>
        </id>
        <property name="sentenceId">
            <column name="sentence_id" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="categoryId">
            <column name="category_id" sql-type="bigint(20) unsigned"/>
        </property>
    </class>
</hibernate-mapping>