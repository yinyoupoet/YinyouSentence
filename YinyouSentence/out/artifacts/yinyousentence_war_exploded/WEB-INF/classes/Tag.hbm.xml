<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Tag" table="tag" schema="yinyousentence">
        <id name="id">
            <column name="id" sql-type="bigint(20) unsigned"/>
            <generator class="increment"></generator>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(25)" length="25"/>
        </property>
        <property name="quoteNum">
            <column name="quote_num" sql-type="bigint(20) unsigned"/>
        </property>
    </class>
</hibernate-mapping>